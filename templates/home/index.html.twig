{% extends 'base.html.twig' %}

{% block title %}scraping des données{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>récupération des données</h1>

    <h3>
    {% if lastDate is not null %}
    La dernière date est le {{ lastDate }}.
    {% else %}
    Il n'y a pas encore d'historique.
    {% endif %}
    </h3>

    {% for row in newData %}
        {% for eachRow in row %}
            <p>{{ eachRow }}</p>
        {% endfor %}
    {% endfor %}

</div>
{% endblock %}
